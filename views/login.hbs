<div class="container">
	<header style="text-align: center;">
        
        <div class="login">
            <form method="POST" action="/account/dologin">
            <h1>Login</h1>
            <hr/>
            <h1>Teacher Login</h1>
            <p><input type="text" id="email" name="email" placeholder="Username"></p>
            <p><input type="password" id="password" name="password" placeholder="Password"></p>
            <p style = " color: red">{{message}}</p>
            <p><button id="btn">Login</button></p>

            </form>
            
        </div>
    </header>
    <script>
        $("#btn").click(function(){
            $.ajax({
                url: '/account/dologin',
                type : "post",
                data:{
                    username : $("#email").val(),
                    password : $("#password").val(),
                }
            })
        });
    </script>
    <br>
